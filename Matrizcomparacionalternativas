fprintf('Incentivos Fiscales: %.2f\n', media_incentivos_fiscales_social);
fprintf('Infraestructura de Carga: %.2f\n', media_infraestructura_carga_social);
fprintf('Promover Uso de Vehículo Eléctrico: %.2f\n', media_promover_vehiculo_electrico_social);
fprintf('Promoción de Electromovilidad: %.2f\n', media_promocion_electromovilidad_social);



%%%Agrupacion de las medias geometricas de alternativas bajo cada
%%% criterio para poder hacer la comparacion de matrices AHP


% Agrupar las medias de alternativas bajo criterio economico en un vector para simplificar las comparaciones
medias_alternativas_economico = [media_incentivos_fiscales_economico, media_infraestructura_carga_economico, media_promover_vehiculo_electrico_economico, media_promocion_electromovilidad_economico];
n = length(medias_alternativas_economico);

% Inicializar la matriz de comparación de criterios
matriz_comparacion = ones(n, n);  % Inicializar con 1s en la diagonal

% Paso 2: Llenar la matriz de comparación utilizando la escala de Saaty
for i = 1:n
    for j = i+1:n
        % Diferencia relativa entre criterios
        diff = abs(medias_alternativas_economico(i) - medias_alternativas_economico(j));
        
       if diff < 0.25
    valor = 1;   % Igual de importantes
elseif diff < 0.75
    valor = 2;   % Muy ligeramente más importante
elseif diff < 1.25
    valor = 3;   % Ligeramente más importante
elseif diff < 1.75
    valor = 4;   % Entre ligeramente y claramente más importante
elseif diff < 2.25
    valor = 5;   % Claramente más importante
elseif diff < 2.75
    valor = 6;   % Entre claramente y mucho más importante
elseif diff < 3.25
    valor = 7;   % Mucho más importante
elseif diff < 4.0
    valor = 8;   % Entre mucho más y extremadamente más importante
else
    valor = 9;   % Extremadamente más importante
end
        
        % Asignar valor en la matriz: si el criterio en la fila es mayor,
        % él recibe el valor, si no, recibe el recíproco
        if medias_alternativas_economico(i) > medias_alternativas_economico(j)
            matriz_comparacion(i, j) = valor;
            matriz_comparacion(j, i) = 1 / valor;
        else
            matriz_comparacion(i, j) = 1 / valor;
            matriz_comparacion(j, i) = valor;
        end
    end
end


% Mostrar la matriz de comparación en la Command Window
disp('Matriz de comparación de alternativas bajo criterio economico:');
disp(array2table(matriz_comparacion, 'VariableNames', {'Incentivos fiscales', 'Infraestructura de carga', 'Promover uso vehiculo electrico', 'Promocion electromovilidad',}, ...
                'RowNames', {'Incentivos fiscales', 'Infraestructura de carga', 'Promover uso vehiculo electrico', 'Promocion electromovilidad'}));

    
% Agrupar las medias de alternativas bajo criterio ambiental en un vector para simplificar las comparaciones
medias_alternativas_ambiental = [media_incentivos_fiscales_ambiental, media_infraestructura_carga_ambiental, media_promover_vehiculo_electrico_ambiental, media_promocion_electromovilidad_ambiental];
n = length(medias_alternativas_ambiental);

% Inicializar la matriz de comparación de criterios
matriz_comparacion = ones(n, n);  % Inicializar con 1s en la diagonal

% Paso 2: Llenar la matriz de comparación utilizando la escala de Saaty
for i = 1:n
    for j = i+1:n
        % Diferencia relativa entre criterios
        diff = abs(medias_alternativas_ambiental(i) - medias_alternativas_ambiental(j));
        
        % Aplicar la escala de Saaty según la diferencia
        if diff < 0.5
            valor = 1;   % Igual de importantes
        elseif diff < 1.0
            valor = 3;   % Ligeramente más importante
        elseif diff < 2.0
            valor = 5;   % Claramente más importante
        elseif diff < 3.0
            valor = 7;   % Mucho más importante
        else
            valor = 9;   % Extremadamente más importante
        end
        
        % Asignar valor en la matriz: si el criterio en la fila es mayor,
        % él recibe el valor, si no, recibe el recíproco
        if medias_alternativas_ambiental(i) > medias_alternativas_ambiental(j)
            matriz_comparacion(i, j) = valor;
            matriz_comparacion(j, i) = 1 / valor;
        else
            matriz_comparacion(i, j) = 1 / valor;
            matriz_comparacion(j, i) = valor;
        end
    end
end


% Mostrar la matriz de comparación en la Command Window
disp('Matriz de comparación de alternativas bajo criterio ambiental:');
disp(array2table(matriz_comparacion, 'VariableNames', {'Incentivos fiscales', 'Infraestructura de carga', 'Promover uso vehiculo electrico', 'Promocion electromovilidad',}, ...
                'RowNames', {'Incentivos fiscales', 'Infraestructura de carga', 'Promover uso vehiculo electrico', 'Promocion electromovilidad'}));


% Agrupar las medias de alternativas bajo criterio tecnico en un vector para simplificar las comparaciones
medias_alternativas_tecnico = [media_incentivos_fiscales_tecnico, media_infraestructura_carga_tecnico, media_promover_vehiculo_electrico_tecnico, media_promocion_electromovilidad_tecnico];
n = length(medias_alternativas_tecnico);

